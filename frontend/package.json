{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i styles/global.css -o styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:schema:generate": "npx prisma format --schema=./prisma/schema.prisma && npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:schema:migrate:dev": "pnpm run prisma:schema:generate && npx prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:schema:push": "npx prisma db push --schema=./prisma/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts",
    "schema": "./prisma/schema.prisma"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.14.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "20.2.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "clsx": "^1.2.1",
    "focus-visible": "^5.2.0",
    "framer-motion": "^10.12.12",
    "next": "13.4.4",
    "next-auth": "^4.22.1",
    "postcss": "8.4.23",
    "postcss-focus-visible": "^8.0.2",
    "prisma": "^4.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0"
  }
}
